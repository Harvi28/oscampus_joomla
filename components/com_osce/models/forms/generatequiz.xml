<?xml version="1.0" encoding="utf-8"?>
<form>
<fieldset 
			name="tags"
		>
		<field
					name="tags"
					class="tags"
					id="tags"
					type="sql"
					query="SELECT id, title FROM #__osce_tags WHERE published=1"
    				key_field="id"
    				value_field = "title"
					multiple="multiple"
					required="true"

		>
				
		</field>
		<fields name="list">
		<field
			name="limit"
			type="limitbox"
			class="input-mini"
			default="25"
			
		/>
	</fields>		
		</fieldset>
</form>